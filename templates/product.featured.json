{
  "name": "Featured Product",
  "sections": {
    "product-navigation": {
      "type": "product-navigation",
      "settings": {
        "show_breadcrumbs": true,
        "show_collection_navigation": true,
        "show_previous_next": true,
        "padding_top": 20,
        "padding_bottom": 20
      }
    },
    "main-product": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "vendor",
          "settings": {
            "show_vendor": true
          }
        },
        "title": {
          "type": "title",
          "settings": {
            "title_size": "large"
          }
        },
        "badges": {
          "type": "badges",
          "settings": {
            "show_sale_badge": true,
            "show_new_badge": true,
            "show_popular_badge": true
          }
        },
        "price": {
          "type": "price",
          "settings": {
            "show_tax_shipping_info": true
          }
        },
        "rating": {
          "type": "rating",
          "settings": {
            "show_rating_count": true
          }
        },
        "inventory": {
          "type": "inventory",
          "settings": {
            "show_inventory_quantity": true,
            "inventory_threshold": 10
          }
        },
        "variant_selector": {
          "type": "variant_selector",
          "settings": {
            "picker_type": "button",
            "enable_color_swatches": true,
            "enable_size_chart": true
          }
        },
        "description_short": {
          "type": "description_short",
          "settings": {
            "word_limit": 50
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true,
            "enable_quantity_selector": true,
            "enable_payment_button": true,
            "show_pickup_availability": true
          }
        },
        "custom_pc_notice": {
          "type": "custom_html",
          "settings": {
            "custom_html": "<div class=\"featured-notice\"><p><strong>Want this PC with custom specs?</strong> Use our <a href=\"/pages/pc-builder\">PC Builder</a> to create a similar system with your preferred components.</p></div>",
            "text_alignment": "left"
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_facebook": true,
            "share_twitter": true,
            "share_pinterest": true
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "badges",
        "price",
        "rating",
        "inventory",
        "variant_selector",
        "description_short",
        "buy_buttons",
        "custom_pc_notice",
        "share"
      ],
      "settings": {
        "enable_sticky_info": true,
        "enable_video_looping": false,
        "enable_image_zoom": true,
        "gallery_layout": "thumbnail_slider",
        "gallery_position": "left",
        "thumbnail_position": "below",
        "mobile_gallery_layout": "full_width",
        "mobile_thumbnail_position": "below",
        "image_ratio": "adapt",
        "padding_top": 40,
        "padding_bottom": 0
      }
    },
    "product-highlights": {
      "type": "product-highlights",
      "blocks": {
        "highlight-0": {
          "type": "highlight",
          "settings": {
            "icon": "cpu",
            "title": "High-Performance CPU",
            "text": "Latest generation processor for exceptional performance"
          }
        },
        "highlight-1": {
          "type": "highlight",
          "settings": {
            "icon": "gpu",
            "title": "Powerful Graphics",
            "text": "Top-tier GPU for stunning visuals and smooth gameplay"
          }
        },
        "highlight-2": {
          "type": "highlight",
          "settings": {
            "icon": "zap",
            "title": "Lightning Fast",
            "text": "Fast memory and storage for responsive computing"
          }
        },
        "highlight-3": {
          "type": "highlight",
          "settings": {
            "icon": "thermometer",
            "title": "Advanced Cooling",
            "text": "Premium cooling solution for optimal temperatures"
          }
        }
      },
      "block_order": [
        "highlight-0",
        "highlight-1",
        "highlight-2",
        "highlight-3"
      ],
      "settings": {
        "title": "Key Features",
        "layout": "grid",
        "columns": 4,
        "show_icons": true,
        "padding_top": 60,
        "padding_bottom": 60
      }
    },
    "product-tabs": {
      "type": "product-tabs",
      "blocks": {
        "description": {
          "type": "description",
          "settings": {
            "title": "Description"
          }
